--- smeserver-manager-0.1.0.old/root/etc/e-smith/web/panels/manager2/html/less/404_500_errors.less	1970-01-01 01:00:00.000000000 +0100
+++ smeserver-manager-0.1.0/root/etc/e-smith/web/panels/manager2/html/less/404_500_errors.less	2018-02-07 00:58:32.401000775 +0100
@@ -0,0 +1,38 @@
+/* 
+    Page: 404 and 500 error pages
+------------------------------------
+*/
+
+.error-page {
+    width: 600px;
+    margin: 20px auto 0 auto;
+    @media screen and (max-width: @screen-sm) {
+        width: 100%;
+    }
+    //For the error number e.g: 404
+    > .headline {
+        float: left;        
+        font-size: 100px;
+        font-weight: 300;
+        @media screen and (max-width: @screen-sm) {
+            float: none;
+            text-align: center;
+        }
+    }
+    //For the message
+    > .error-content {
+        margin-left: 190px;
+        @media screen and (max-width: @screen-sm) {
+            margin-left: 0;
+        }
+        > h3 {
+            font-weight: 300;
+            font-size: 25px;
+            @media screen and (max-width: @screen-sm) {
+                text-align: center;
+            }
+        }
+        display: block;
+    }
+    .clearfix();
+}
