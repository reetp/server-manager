+    > li.active > a {
+        font-weight: 600;
+    }
+    > li > a:hover {
+        background-color: #f6f6f6;
+    }
+
+    &.nav-stacked {
+        > li > a {
+            border-top: 0;
+            border-left: 3px solid transparent;
+            .border-radius(0);
+            color: #444;
+        }
+        > li.active > a, > li.active > a:hover {
+            background-color: #f6f6f6;
+            border-left-color: @light-blue;
+            color: #444;
+        }
