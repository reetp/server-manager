+            border-top: 3px solid transparent;
+            margin-bottom: -2px;
+            > a {
+                .border-radius(0)!important;
+                &, &:hover {
+                    background: transparent;
+                    margin: 0;
+                }
+            }
+            &:not(.active) {
+                > a:hover, > a:focus, > a:active {
+                    border-color: transparent;
+                }
+            }
+            margin-right: 5px;
