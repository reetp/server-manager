+                border-left-color: #f4f4f4;
+                border-right-color: #f4f4f4;
+            }
+
+        }
+
+        > li:first-of-type {
+            margin-left: 0;
+            &.active {
+                > a {
+                    border-left-width: 0;
+                }
+            }
+        }
+
+        //Pulled to the right
+        &.pull-right {
+            float: none!important;
+            > li {
+                float: right;
+            }
+            > li:first-of-type {
+                margin-right: 0;
+                &.active {
+                    > a {
+                        border-left-width: 1px;
+                        border-right-width: 0;
+                    }
+                }
+            }
+        }
+
+        > li.header {
+            font-weight: 400;
+            line-height: 35px;            
+            padding: 0 10px;
+            font-size: 20px;
+            color: #444;
+            cursor: default;
+            > .fa, > .glyphicon, > .ion {
+                margin-right: 10px;
+            }
+        }
+    }
+
+    > .tab-content {
+        background: #fff;
+        padding: 10px;
+    }
+}
+
+/* Nav tabs bottom */
+.tabs-bottom {
+    
+    &.nav-3 {
+        li a {
+            width: percentage(100/3)!important;
+        }
+    }
+    
+    li a {
+        border: 0;
+    }
+}
+
+/* PAGINATION */
+.pagination {
+    > li > a {
+        background: #fafafa;
+        color: #666;
